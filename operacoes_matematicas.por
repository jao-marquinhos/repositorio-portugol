programa {
  funcao inicio() {
    inteiro n1, n2

    escreva("Me diga um número: ")
    leia(n1)
    escreva("Me diga outro número: ")
    leia(n2)

    escreva("Assim seria a soma desses dois números: ", n1+n2, "\n")
    escreva("Assim seria a subtração desses dois números: ", n1-n2, "\n")
    escreva("Assim seria a multiplicação desses dois números: ", n1*n2, "\n")
    escreva("Assim seria a divisão desses dois números: ", n1/n2)
  }
}
